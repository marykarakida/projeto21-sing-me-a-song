// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Recommendation {
  id          Int    @id @default(autoincrement())
  name        String @unique
  youtubeLink String
  score       Int    @default(0)

  @@map("recommendations")
}
